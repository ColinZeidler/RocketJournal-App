<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        class="zeidler.colin.rocketjournal.dataviews.flightlog.FlightLogListFragment"
        android:id="@+id/flightlog_list_fragment"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_flightlog_list" />
    <fragment
        class="zeidler.colin.rocketjournal.dataviews.flightlog.details.FlightLogDetailFragment"
        android:id="@+id/flightlog_details_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_flightlog_view" />

</LinearLayout>